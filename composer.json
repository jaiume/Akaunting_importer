{
    "name": "akaunting/importer",
    "description": "Bank statement and CSV import tool for Akaunting installations",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "php-di/php-di": "^6.1",
        "php-di/slim-bridge": "^3.4",
        "phpmailer/phpmailer": "^6.9",
        "slim/psr7": "^1.8",
        "slim/slim": "^4.12",
        "slim/twig-view": "^3.4",
        "smalot/pdfparser": "^2.12",
        "twig/twig": "^3.8",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
